{
  "name": "filtdevtools",
  "version": "0.0.0",
  "main": "electron/main.js",
  "scripts": {
    "start": "npm-run-all --parallel build electron",
    "start-front": "ng serve",
    "build": "ng build --base-href ./",
    "electron": "electron . --no-sandbox",
    "electron-dev": "cross-env NODE_ENV=development electron . --no-sandbox",
    "wait-on": "wait-on http://localhost:4200",
    "start-dev": "npm-run-all --parallel start-front wait-on electron-dev"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.0.0",
    "@angular/common": "^19.0.0",
    "@angular/compiler": "^19.0.0",
    "@angular/core": "^19.0.0",
    "@angular/forms": "^19.0.0",
    "@angular/platform-browser": "^19.0.0",
    "@angular/platform-browser-dynamic": "^19.0.0",
    "@angular/router": "^19.0.0",
    "@primeng/themes": "^19.0.0",
    "electron": "^16.0.0",
    "npm-run-all": "^4.1.5",
    "primeicons": "^7.0.0",
    "primeng": "^19.0.0",
    "rxjs": "~7.8.0",
    "sqlite3": "^5.1.7",
    "systeminformation": "^5.25.11",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-builders/custom-webpack": "^19.0.0",
    "@angular-devkit/build-angular": "^19.0.6",
    "@angular/cli": "^19.0.6",
    "@angular/compiler-cli": "^19.0.0",
    "@types/jasmine": "~5.1.0",
    "cross-env": "^7.0.3",
    "jasmine-core": "~5.4.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.6.2",
    "wait-on": "^8.0.2"
  }
}
